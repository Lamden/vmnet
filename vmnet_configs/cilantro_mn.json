{
    "aws": {
        "security_group": {
            "name": "simple_https",
            "description": "Simple https security group",
            "permissions": [
              {"IpProtocol": "tcp",
               "FromPort": 443,
               "ToPort": 443,
               "IpRanges": [{"CidrIp": "0.0.0.0/0"}]},
              {"IpProtocol": "tcp",
               "FromPort": 22,
               "ToPort": 22,
               "IpRanges": [{"CidrIp": "0.0.0.0/0"}]}
            ]
        },
        "images": [{
            "name": "cilantro_base",
            "ami": "ami-0f65671a86f061fcd",
            "repo_name": "cilantro",
            "repo_url": "https://github.com/Lamden/cilantro.git",
            "branch": "dev",
            "username": "ubuntu",
            "instance_type": "t2.large",
            "environment": {}
        }]
    },
    "services": [
        {
            "name": "sample-node",
            "image": "cilantro_base",
            "count": 1,
            "ports": [
                8080
            ]
        }
    ]
}
